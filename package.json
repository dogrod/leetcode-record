{
  "name": "leetcode-record",
  "version": "1.0.0",
  "description": "Record of leetcord problem solutions",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dogrod/leetcode-record.git"
  },
  "keywords": [
    "leetcode"
  ],
  "author": "dogrod <rodrickjue@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dogrod/leetcode-record/issues"
  },
  "homepage": "https://github.com/dogrod/leetcode-record#readme",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "ts-jest": "^22.0.1",
    "typescript": "^2.6.2"
  }
}
